  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:logger": "jest logger.test.ts",
    "test:performance": "jest --testNamePattern='Performance|Event Loop'",
    "test:integration": "jest --testNamePattern='NestJS'",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "npm run typeorm migration:run -d src/config/typeorm.config.ts",
    "dynamo:seed": "awslocal dynamodb batch-write-item --request-items file:///home/stephanyes/Desktop/Backend/nestjs-aws-dynamodb/databases/DYNAMO/AWS-JSON-FILES/dynamo_books_seed.json",
    "dynamo:table": "awslocal dynamodb create-table --cli-input-json file:///home/stephanyes/Desktop/Backend/nestjs-aws-dynamodb/databases/DYNAMO/AWS-JSON-FILES/create-table.json",
    "postgres:seed": "docker exec -i nestjs-aws-dynamodb-postgres-1 psql -U postgres -d test < /home/stephanyes/Desktop/Backend/nestjs-aws-dynamodb/databases/POSTGRES/seed.sql"
  },
